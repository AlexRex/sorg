= content main
  .seq-flex
    .seq-words
      .seq-words-inner
        h1 {{.Photo.Title}}
        .seq-divider
        {{HTML .Description}}
    .seq-photograph
      img src="/seq/photographs/{{.SeqName}}/{{QueryEscape .Photo.Slug}}_large.jpg" srcset="/seq/photographs/{{.SeqName}}/{{QueryEscape .Photo.Slug}}_large@2x.jpg 2x, /seq/photographs/{{.SeqName}}/{{QueryEscape .Photo.Slug}}_large.jpg 1x"
